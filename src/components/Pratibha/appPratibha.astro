---
import { getEntry } from "astro:content";
const appPratibha = (await getEntry("pratibha", "apppratibha")).data;
---

<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Kiran Pratibha 2024</title>
        <link rel="stylesheet" href="/assets/css/Pratibha/appPratibha.css" />
    </head>
    <body>
        <div class="container app-component-c1" style="margin-top: 150px;">
            <div class="app-left-container">
                {appPratibha.title}
            </div>
            <button class="app-download-button"
                >{appPratibha.downloadAppbtn}</button
            >
             <img src="/assets/images/programs/Groupicon.png" alt="GroupIcon" class="group-icon">
            <img src={appPratibha.image1} alt="Hero Image 1" class="image1" />
            <img src={appPratibha.image2} alt="Hero Image 2" class="image2" />
            <img src={appPratibha.image3} alt="Hero Image 3" class="image3" />
            <img src={appPratibha.image4} alt="Hero Image 4" class="image4" />
        </div>
    </body>
</html>
