---
import SectionTop from "./SectionTop.astro";
const {sec="certificateSection" ,title, image="/assets/images/financial/group1.jpg",className } = Astro.props;
---

<section class=`max-w-[1296px] mx-auto ${className}`>


  <SectionTop sec={sec} title={title}/>

  <div class="w-full h-auto sm:h-[500px] md:h-[600px] lg:h-[766px]  mb-[120px]">
    <img 
      src={image}  
      class="w-full h-full object-cover rounded-3xl" 
    />
  </div>
  
  
</section>
