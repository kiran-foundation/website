---
import "aos/dist/aos.css";
import "jarallax/dist/jarallax.min.css";
import Hero from "../../components/volunteer/Hero.astro";
import Header from "../../components/common/Header.astro";
import AboutVolunteerRole from "../../components/volunteer/AboutVolunteerRole.astro";
import Form from "../../components/volunteer/Form.astro";
import Layout from "../../layouts/Layout.astro";
interface Props {
  title: string;
}
---

<Layout title="Intenrship Roles">
  <main class="page-wrapper">
    <Header />
    <Hero />
    <AboutVolunteerRole />
    <Form />
  </main>
  <a
    class="btn-scroll-top"
    href="#top"
    data-scroll
    aria-label="Scroll back to top"
  >
    <svg
      viewBox="0 0 40 40"
      fill="currentColor"
      xmlns="http://www.w3.org/2000/svg"
    >
      <circle
        cx="20"
        cy="20"
        r="19"
        fill="none"
        stroke="currentColor"
        stroke-width="1.5"
        stroke-miterlimit="10"></circle>
    </svg>
    <i class="ai-arrow-up"></i>
  </a>
</Layout>
