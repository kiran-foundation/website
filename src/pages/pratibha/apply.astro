---
import "aos/dist/aos.css"; // Importing AOS (Animate On Scroll) CSS
import "jarallax/dist/jarallax.min.css"; // Importing Jarallax CSS for parallax effects
import Layout from "../../layouts/Layout.astro"; // Layout component
import ComingSoon from "../../components/coming-soon/ComingSoon.astro";
import Breadcrumb from "../../components/common/Breadcrumb.astro";



---

<!-- Inline script for theme initialization -->
<script src="/assets/js/theme.min.js" is:inline></script>

<!-- AOS (Animate On Scroll) initialization -->
<script>
  import AOS from "aos";
  AOS.init();
</script>

<!-- External scripts for additional functionality -->
<script src="jarallax/dist/jarallax.min.js"></script>
<script src="simplebar/dist/simplebar.min.js"></script>

<Layout title="Pratibha-App;y">
  <main class="page-wrper w-full">
    <Breadcrumb/>
    <ComingSoon/>
    </>
    <!-- Scroll to top button -->
    <a
      class="btn-scroll-top"
      href="#top"
      data-scroll
      aria-label="Scroll back to top"
    >
      <svg
        viewBox="0 0 40 40"
        fill="currentColor"
        xmlns="http://www.w3.org/2000/svg"
      >
        <circle
          cx="20"
          cy="20"
          r="19"
          fill="none"
          stroke="currentColor"
          stroke-width="1.5"
          stroke-miterlimit="10"
        ></circle>
      </svg>
      <i class="ai-arrow-up"></i>
    </a>
  </main>
</Layout>
