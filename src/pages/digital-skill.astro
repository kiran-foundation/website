---
import "aos/dist/aos.css";
import "jarallax/dist/jarallax.min.css";
import Layout from "../layouts/Layout.astro";
import Hero from "../components/digital-skill/Hero.astro";
import Programe from "../components/digital-skill/Programe.astro";
import Course from "../components/digital-skill/Course.astro";
import Video from "../components/digital-skill/Video.astro";
import VideoPrograme from "../components/digital-skill/VideoPrograme.astro";
import Heading from "../components/common/heading.astro";
import TestimonialsCarousel from "../components/common/TestimonialsCarousel.astro";
import OurTraniers from "../components/digital-skill/OurTraniers.astro";
import Join from "../components/digital-skill/Join.astro";

---


<Layout>
  <main class="page-wrapper">
    <Hero/>
     <Programe/>
    <Course/>
    <Heading title="Testimonials"/>
    <TestimonialsCarousel pageTag="pratibha"/>
    <Video/>
    <VideoPrograme/>
    <OurTraniers/>
    <Join/>
</main>

<!-- Back to top button -->
<a
class="btn-scroll-top"
href="#top"
data-scroll
aria-label="Scroll back to top"
>
<svg
  viewBox="0 0 40 40"
  fill="currentColor"
  xmlns="http://www.w3.org/2000/svg"
>
  <circle
    cx="20"
    cy="20"
    r="19"
    fill="none"
    stroke="currentColor"
    stroke-width="1.5"
    stroke-miterlimit="10"></circle>
  </svg>
  <i class="ai-arrow-up"></i>
</a>
</Layout>