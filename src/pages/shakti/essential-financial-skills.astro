---
import "aos/dist/aos.css";
import "jarallax/dist/jarallax.min.css";
import Layout from '../../layouts/Layout.astro';
import Breadcrumb from "../../components/common/Breadcrumb.astro";
import Hero from "../../components/common/Hero.astro";
import TestimonialComponent from "../../components/common/TestimonialComponent.astro";
import PageBottom from "../../components/shakti/PageBottom.astro";
import TestimonialsCarousel from "../../components/common/TestimonialsCarousel.astro";
import Videotestomonial from "../../components/shakti/videotestomonial.astro";
import Mentors from "../../components/shakti/Mentors.astro";
---

<Layout title="financial skills"
>
  <main class="page-wrapper ">
    <Breadcrumb hidden={false} />
    <Hero collection="shakti" slug="financial-skills/hero" />
    <img src="/assets/images/financial/investments-visual-1.png" class="max-w-[1260px] mx-auto  pt-40 " width="920px"  />
    <PageBottom image="/assets/images/financial/financial-certificate.png" title="Learn for Free and Get Your Certificate" sec="certificateSection" className=" mt-[164px]"/>
    <Videotestomonial className="mb-[164px]"/>
    <Mentors />
    <TestimonialsCarousel pageTag="shakti" />
    <PageBottom title="Make Decisions with Knowledge and Help Build  a Strong and Resilient Society" sec="bottomSection" className=" mt-[164px]"/>
  </main>
</Layout>

<!-- Back to top button -->
<a
class="btn-scroll-top"
href="#top"
data-scroll
aria-label="Scroll back to top"
>
<svg
  viewBox="0 0 40 40"
  fill="currentColor"
  xmlns="http://www.w3.org/2000/svg"
>
  <circle
    cx="20"
    cy="20"
    r="19"
    fill="none"
    stroke="currentColor"
    stroke-width="1.5"
    stroke-miterlimit="10"></circle>
</svg>
<i class="ai-arrow-up"></i>
</a>

