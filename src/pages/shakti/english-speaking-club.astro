---
import "aos/dist/aos.css";
import "jarallax/dist/jarallax.min.css";
import Layout from '../../layouts/Layout.astro';
import Breadcrumb from "../../components/common/Breadcrumb.astro";
import Hero from "../../components/common/Hero.astro";
import PageBottom from "../../components/shakti/PageBottom.astro";
import TestimonialsCarousel from "../../components/common/TestimonialsCarousel.astro";
import Videotestomonial from "../../components/shakti/videotestomonial.astro";
import Mentors from "../../components/shakti/Mentors.astro";
import WhyFinancial from "../../components/shakti/WhyFinancial.astro";
import CourseDetail from "../../components/shakti/CourseDetail.astro";
import WhatWeWill from "../../components/shakti/WhatWeWill.astro";
import AfterTheCourse from "../../components/shakti/AfterTheCourse.astro";
import PlainImage from "../../components/shakti/PlainImage.astro";
import MessageComponent from "../../components/shakti/MessageComponent.astro";
import ComingSoon from "../../components/coming-soon/ComingSoon.astro";
---

<Layout title="financial skills"
>
  <ComingSoon/>
</Layout>

<!-- Back to top button -->
<a
class="btn-scroll-top"
href="#top"
data-scroll
aria-label="Scroll back to top"
>
<svg
  viewBox="0 0 40 40"
  fill="currentColor"
  xmlns="http://www.w3.org/2000/svg"
>
  <circle
    cx="20"
    cy="20"
    r="19"
    fill="none"
    stroke="currentColor"
    stroke-width="1.5"
    stroke-miterlimit="10"></circle>
</svg>
<i class="ai-arrow-up"></i>
</a>

