---
import "aos/dist/aos.css";
import "jarallax/dist/jarallax.min.css";
import Layout from "../layouts/Layout.astro";
import Header from "../components/common/Header.astro";
import Application from "../components/pratibha/application.astro";
import Footer from "../components/common/Footer.astro";
import FaqPratibha from "../components/pratibha/FaqPratibha.astro";
import AppPratibha from "../components/pratibha/appPratibha.astro";
interface Props {
  title: string;
}
---

<Layout>
  <!-- Page wrapper -->
  <main class="page-wrapper">
    <Header />
    <Application />
    <FaqPratibha />
    <div class="space"></div>
    <div class="space"></div>
    <div class="space"></div>
    <div class="space"></div>
    <AppPratibha />
    <div class="space"></div>
    <Footer />
  </main>

  =
</Layout>

<!-- Back to top button -->
<a
  class="btn-scroll-top"
  href="#top"
  data-scroll
  aria-label="Scroll back to top"
>
  <svg
    viewBox="0 0 40 40"
    fill="currentColor"
    xmlns="http://www.w3.org/2000/svg"
  >
    <circle
      cx="20"
      cy="20"
      r="19"
      fill="none"
      stroke="currentColor"
      stroke-width="1.5"
      stroke-miterlimit="10"></circle>
  </svg>
  <i class="ai-arrow-up"></i>
</a>

<Layout />
